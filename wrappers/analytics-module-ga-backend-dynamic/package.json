{
  "name": "analytics-module-ga-backend-dynamic",
  "version": "0.0.1",
  "description": "Dynamic Wrapper for the backstage analytics module backend plugin",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "private": true,
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "backstage": {
    "role": "backend-plugin"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "backstage-cli package build",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test --passWithNoTests --coverage",
    "clean": "backstage-cli package clean",
    "clean-dynamic-sources": "yarn clean && rm -Rf node_modules",
    "export-dynamic": "janus-cli package export-dynamic-plugin --embed-package @backstage/plugin-analytics-module-ga"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaeltuelho/backstage-dynamic-plugins.git"
  },
  "keywords": [
    "backstage",
    "analytics",
    "rhdh",
    "wrapper",
    "plugin",
    "red hat developer hub"
  ],
  "author": "Rafael T. C. Soares",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rafaeltuelho/backstage-dynamic-plugins/issues"
  },
  "homepage": "https://github.com/rafaeltuelho/backstage-dynamic-plugins#readme",
  "dependencies": {
    "@backstage/backend-dynamic-feature-service": "^0.2.2",
    "@backstage/plugin-analytics-module-ga": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.25.2",
    "@janus-idp/cli": "^1.7.4"
  },
  "files": [
    "dist",
    "dist-dynamic/*.*",
    "dist-dynamic/dist/**",
    "dist-dynamic/alpha/*"
  ]
}
